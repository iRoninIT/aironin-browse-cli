{
  "name": "aironin-browse-cli",
  "version": "1.1.0",
  "description": "aiRonin Browse CLI tool with headed Chrome support",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "aironin-browse": "dist/cli.js"
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist node_modules"
